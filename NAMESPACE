import(stats, nlme, drc, ggplot2)
importFrom("statmod", gauss.quad.prob)
importFrom("plyr", ldply)
importFrom("gtools", combinations)
export(medrm, glsdrm, mmaED, mmaBMD, LL4.loged, LN4.loged, W14.loged, W24.loged, EDcond, EDmarg, SImarg, BMD, BMDmarg, mmplot)

S3method(df.residual, medrc)
S3method(plot, medrc)
S3method(predict, medrc)
S3method(vcov, medrc)
S3method(summary, medrc)
